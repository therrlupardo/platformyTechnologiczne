<Window x:Class="laboratorium_10.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:laboratorium_10"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <ToolBar HorizontalAlignment="Left" Height="30" VerticalAlignment="Top" Width="782">
            <Label>Search</Label>
        </ToolBar>
        <DataGrid Name="dataGridView1" HorizontalAlignment="Left" Height="276" Margin="41,45,0,0"
                  VerticalAlignment="Top" Width="440" CanUserResizeColumns="False" 
                  AutoGenerateColumns="False">
            <DataGrid.Columns>
                <DataGridTextColumn Header="Model" Binding="{Binding model}"/>
                <DataGridTextColumn Header="Motor" Binding="{Binding motor}"/>
                <DataGridTextColumn Header="Year" Binding="{Binding year}"/>
            </DataGrid.Columns>
        </DataGrid>
    </Grid>
</Window>
